<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="cacheEnabled" value="false" />
	</settings>
	
	<typeAliases>		
		<typeAlias type="com.test.matching.matching.model.vo.User" alias="User" />
		<typeAlias type="com.test.matching.matching.model.vo.Matching" alias="Matching" />

	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/matching-mapper.xml" />
		<mapper resource="mappers/user-mapper.xml" />
	</mappers>
</configuration>
