<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="cacheEnabled" value="false" />
	</settings>
	
	<typeAliases>		
		<typeAlias type="com.test.matching.user.model.vo.User" alias="User" />
		<typeAlias type="com.test.matching.notice.model.vo.Notice" alias="Notice" />
		<typeAlias type="com.test.matching.common.SearchDate" alias="SearchDate" />
		<typeAlias type="com.test.matching.common.Paging" alias="Paging" />
		<typeAlias type="com.test.matching.community.model.vo.Community" alias="Community" />
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/user-mapper.xml" />
		<mapper resource="mappers/notice-mapper.xml" />
		<mapper resource="mappers/community-mapper.xml" />
	</mappers>
</configuration>
